{
    "version": 2,
    "builds": [
      {
        "src": "api/generate.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/api/generate",
        "dest": "/api/generate.go"
      },
      {
        "src": "/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  